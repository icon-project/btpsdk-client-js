{
  "name": "btp",
  "version": "1.0.0",
  "description": "",
  "main": "./lib.cjs/index.js",
  "module": "./lib.esm/index.js",
  "scripts": {
    "watch": "tsc --project ./config/tsconfig.esm.json --watch",
    "build1": "npm run clean && npm run build:all && npm run ",
    "build": "tsc --project ./config/tsconfig.cjs.json",
    "build:all": "npm run build:cjs && npm run build:esm",
    "build:watch": "npm run build:esm -- -w",
    "build:cjs": "tsc --project ./config/tsconfig.cjs.json",
    "build:esm": "tsc --project ./config/tsconfig.esm.json",
    "dist": "rollup -c ./config/rollup.config.js && uglifyjs ./dist/btp.js -o ./dist/btp.min.js && gzip -k9f ./dist/btp.min.js",
    "clean": "rm -rf dist",
    "test": "mocha",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.31",
    "@types/ws": "^8.5.7",
    "husky": "^8.0.0",
    "mocha": "^10.2.0",
    "rollup": "^3.28.1",
    "ts-node": "^10.9.1",
    "typescript": "5.0.4",
    "ws": "^8.14.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "tslib": "^2.6.2"
  },
  "browser": {
    "assert": false,
    "./lib.esm/provider/fetch.js": "./lib.esm/provider/fetch-browser.js",
    "./lib.esm/provider/ws.js": "./lib.esm/provider/ws-browser.js"
  },
  "exports": {
    ".": {
      "import": "./lib.esm/index.js",
      "default": "./lib.cjs/index.js"
    },
    "./error": {
      "import": "./lib.esm/error/index.js",
      "default": "./lib.cjs/error/index.js"
    },
    "./provider": {
      "import": "./lib.esm/provider/index.js",
      "default": "./lib.cjs/provider/index.js"
    },
    "./service": {
      "import": "./lib.esm/service/index.js",
      "default": "./lib.cjs/service/index.js"
    },
    "./signer": {
      "import": "./lib.esm/signer/index.js",
      "default": "./lib.cjs/signer/index.js"
    },
    "./utils": {
      "import": "./lib.esm/utils/index.js",
      "default": "./lib.cjs/utils/index.js"
    }
  }
}
